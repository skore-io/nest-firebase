{
  "name": "nest-firebase",
  "version": "0.0.1",
  "description": "",
  "author": "Skore",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@golevelup/nestjs-discovery": "^2.2.3",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@testdeck/jest": "0.0.6",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "firebase-functions": "^3.5.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "rootDir": ".",
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "setupFiles": [
      "./test/setup.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverageFrom": [
      "src/**/{!(app.module|index),}.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "json",
      "lcovonly",
      "text",
      "clover"
    ]
  }
}
